    <!-- Joyful Mysteries -->
    <string name="joyful_1_title">L\'Annonciation</string>
    <string name="joyful_1_desc">L\'ange Gabriel annonce a Marie qu\'elle concevra le Fils de Dieu.</string>
    <string name="joyful_2_title">La Visitation</string>
    <string name="joyful_2_desc">Marie rend visite a sa cousine Elisabeth, enceinte de Jean-Baptiste.</string>
    <string name="joyful_3_title">La Nativite</string>
    <string name="joyful_3_desc">Jesus nait a Bethleem.</string>
    <string name="joyful_4_title">La Presentation au Temple</string>
    <string name="joyful_4_desc">Marie et Joseph presentent Jesus au Temple.</string>
    <string name="joyful_5_title">Le Recouvrement au Temple</string>
    <string name="joyful_5_desc">Marie et Joseph retrouvent Jesus enseignant au Temple.</string>

    <!-- Sorrowful Mysteries -->
    <string name="sorrowful_1_title">L\'Agonie au Jardin</string>
    <string name="sorrowful_1_desc">Jesus prie au Jardin de Gethsemani.</string>
    <string name="sorrowful_2_title">La Flagellation</string>
    <string name="sorrowful_2_desc">Jesus est flagelle par les soldats romains.</string>
    <string name="sorrowful_3_title">Le Couronnement d\'Epines</string>
    <string name="sorrowful_3_desc">Jesus est couronne d\'epines et tourne en derision.</string>
    <string name="sorrowful_4_title">Le Portement de la Croix</string>
    <string name="sorrowful_4_desc">Jesus porte sa croix vers le Calvaire.</string>
    <string name="sorrowful_5_title">La Crucifixion</string>
    <string name="sorrowful_5_desc">Jesus meurt sur la croix pour notre salut.</string>

    <!-- Glorious Mysteries -->
    <string name="glorious_1_title">La Resurrection</string>
    <string name="glorious_1_desc">Jesus ressuscite d\'entre les morts le troisieme jour.</string>
    <string name="glorious_2_title">L\'Ascension</string>
    <string name="glorious_2_desc">Jesus monte au Ciel.</string>
    <string name="glorious_3_title">La Pentecote</string>
    <string name="glorious_3_desc">L\'Esprit Saint descend sur les Apotres a la Pentecote.</string>
    <string name="glorious_4_title">L\'Assomption</string>
    <string name="glorious_4_desc">Marie est elevee au Ciel en corps et en ame.</string>
    <string name="glorious_5_title">Le Couronnement de Marie</string>
    <string name="glorious_5_desc">Marie est couronnee Reine du Ciel et de la Terre.</string>

    <!-- Luminous Mysteries -->
    <string name="luminous_1_title">Le Bapteme au Jourdain</string>
    <string name="luminous_1_desc">Jesus est baptise par Jean dans le Jourdain.</string>
    <string name="luminous_2_title">Les Noces de Cana</string>
    <string name="luminous_2_desc">Jesus accomplit son premier miracle aux noces.</string>
    <string name="luminous_3_title">L\'Annonce du Royaume</string>
    <string name="luminous_3_desc">Jesus proclame le Royaume de Dieu et appelle a la conversion.</string>
    <string name="luminous_4_title">La Transfiguration</string>
    <string name="luminous_4_desc">Jesus est transfigure sur le Mont Thabor.</string>
    <string name="luminous_5_title">L\'Institution de l\'Eucharistie</string>
    <string name="luminous_5_desc">Jesus institue l\'Eucharistie lors de la Derniere Cene.</string>
